public class PlusOne6{
    public int[] plusOne(int[] digits) {
        int[] res = new int[digits.length+1];
        int c = 1;
        for (int i=digits.length-1; i>=0; i--) {
            int s = digits[i] + c;
            c = s / 10;
            res[i+1] = s % 10;
        }
        res[0] = c;
        return (res[0] != 0) ? res : Arrays.copyOfRange(res, 1, res.length);
    }
